{
    "version": 2,
    "builds": [
        {
            "src": "/api/index.php",
            "use": "vercel-php@0.6.0"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|images|fonts|favicon.ico)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Cuisine Saisonni√®re",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://cuisine-saisonniere.vercel.app",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stack",
        "SESSION_DRIVER": "array"
    }
}
