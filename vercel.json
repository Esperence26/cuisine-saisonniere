{
    "version": 2,
    "framework": null,
    "builds": [
        {
            "src": "api/index.php",
            "use": "vercel-php@0.6.0"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|images|fonts|build)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Cuisine Saisonni√®re",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "${VERCEL_URL}",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "LOG_LEVEL": "info",
        "SESSION_DRIVER": "array",
        "VIEW_COMPILED_PATH": "/tmp",
        "NODE_PATH": "node"
    }
}
